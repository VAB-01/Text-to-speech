   "source": [
    "import pyttsx3 as pt\n",
    "with open(\"myfile.txt\",\"r\") as file:\n",
    "    lines = file.readlines()\n",
    "speaker = pt.init()\n",
    "for line in lines:\n",
    "    speaker.say(line.strip())\n",
    "    speaker.runAndWait()\n"
   ]
